graph = new Graph()
graph.addNode("c")
graph.addNode("e")
graph.addNode("d")
graph.addNode("g")
graph.addNode("f")
graph.addNode("h")

graph.addEdge("c","d",3)
graph.addEdge("c","e",2)
graph.addEdge("e","d",1)
graph.addEdge("d","f",4)
graph.addEdge("f","h",1)
graph.addEdge("g","h",2)
graph.addEdge("e","g",3)
graph.addEdge("f","g",2)
graph.addEdge("e","f",2)

ksp(graph, 'c', 'h', 3)